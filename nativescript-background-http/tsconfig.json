{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"declaration": false,
		"noImplicitUseStrict": true,
		"experimentalDecorators": true,
		"lib": [
			"es2016"
		]
	},
	"exclude": [
		"node_modules",
		"package"
	]
}